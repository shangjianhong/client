<?xml version="1.0" encoding="UTF-8"?>  
<rules>
    <rulebean>
        <ruleName>addUserProfile</ruleName>
        <dataType>SET</dataType>
        <noNull>NO_NULL</noNull>
        <rulebean><!-- 与uid等值 -->
             <ruleName>_id</ruleName>
             <dataType>LONG</dataType>
             <noNull>CAN_NULL</noNull>
     	</rulebean>

     	<rulebean><!-- 个人设置信息 -->
            <ruleName>pri_info</ruleName>
            <dataType>SET</dataType>
            <noNull>NO_NULL</noNull>     	    
	        <rulebean><!-- 隐身标志位 -->
	            <ruleName>show_flag</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>SYS_VALUE</noNull>
	             <defaultValue>0</defaultValue>
	     	</rulebean>
	        <rulebean><!-- email -->
	            <ruleName>email</ruleName>
	             <dataType>STRING</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	        <rulebean><!-- 允许查看的等级 -->
	            <ruleName>allow_vip</ruleName>
	             <dataType>LONG</dataType>	            
	             <noNull>SYS_VALUE</noNull>
	             <defaultValue>0</defaultValue>
	     	</rulebean>	        	        
	    </rulebean>     
	    	
     	<rulebean><!-- 个人通用返回信息 -->
            <ruleName>basic_info</ruleName>
            <dataType>SET</dataType>
            <noNull>NO_NULL</noNull>     	    
	        <rulebean>
	             <ruleName>uid</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	        <rulebean>
	             <ruleName>nick_name</ruleName>
	             <dataType>STRING</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	        <rulebean><!-- 速约号 -->
	             <ruleName>account</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	        <rulebean><!-- 生日 -->
	             <ruleName>birth</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>	     	     	
	        <rulebean><!-- 主头像 -->
	             <ruleName>main_photo</ruleName>
	             <dataType>STRING</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	        <rulebean><!-- 头像列表，顺序排列  -->
	             <ruleName>photos</ruleName>
	             <dataType>ARRAY</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>	     	
	        <rulebean>
	            <ruleName>last_modify_date</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>SYS_VALUE</noNull>
	             <defaultValue>SystemSecond</defaultValue>
	     	</rulebean>
	        <rulebean> 
	            <ruleName>reg_date</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>SYS_VALUE</noNull>
	             <defaultValue>SystemSecond</defaultValue>
	     	</rulebean>
	        <rulebean><!-- vip等级 -->
	            <ruleName>vip</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>SYS_VALUE</noNull>
	             <defaultValue>0</defaultValue>
	     	</rulebean>
	        <rulebean><!-- 允许浏览的等级 -->
	            <ruleName>allow_vip</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>SYS_VALUE</noNull>
	             <defaultValue>0</defaultValue>
	     	</rulebean>	     	
	        <rulebean><!-- VIP结束时间 -->
	            <ruleName>vip_end</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>CAN_NULL</noNull>
	     	</rulebean>
	        <rulebean><!-- 签名 -->
	             <ruleName>profile_desc</ruleName>
	             <dataType>STRING</dataType>
	             <noNull>CAN_NULL</noNull>
	     	</rulebean>     	
	        <rulebean>
	             <ruleName>age</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>      	     	
	        <rulebean><!-- 星座  -->
	             <ruleName>constellation</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>     	     	    
	        <rulebean>
	             <ruleName>sex</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>          
	        <rulebean>
	             <ruleName>tall</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	     	<!-- 性格：  0:开放   1:保守   2:中性  -->
	        <rulebean>
	             <ruleName>character</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>
	     	<!-- 性取向：  0:异性   1:同性   2:不限  -->
	        <rulebean>
	             <ruleName>sex_orient</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>NOT_NULL</noNull>
	     	</rulebean>      	
	     	<!-- 职业：  0:学生   1:计算机   2:艺术 ... -->
	        <rulebean>
	             <ruleName>job</ruleName>
	             <dataType>LONG</dataType>
	             <noNull>CAN_NULL</noNull>
	     	</rulebean> 		  
	     	<!-- 爱好：  0:听歌唱歌   1:睡觉   2:美食  -->
	        <rulebean>
	             <ruleName>hoby</ruleName>
	             <dataType>ARRAY</dataType>
	             <noNull>CAN_NULL</noNull>
	     	</rulebean> 
	     	<!-- 约会目的：  0:听歌唱歌   1:睡觉   2:美食  -->
	        <rulebean>
	             <ruleName>date_intent</ruleName>
	             <dataType>ARRAY</dataType>
	             <noNull>CAN_NULL</noNull>
	     	</rulebean>
	     	<!-- 约会地点：  0:咖啡厅   1:西餐厅   2:KTV  -->
	        <rulebean>
	             <ruleName>date_location</ruleName>
	             <dataType>ARRAY</dataType>
	             <noNull>CAN_NULL</noNull>
	     	</rulebean>
     	</rulebean>
    </rulebean>    
